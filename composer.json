{
    "name": "the-limbo/container-contracts",
    "type": "library",
    "description": "Generic abstractions for dependency injection container.",
    "keywords": [
        "the-limbo",
        "contracts",
        "container",
        "di",
        "psr-11"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "dr0n1k",
            "email": "a.dronov4job@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Limbo\\Contracts\\Container\\": "./src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "psr/container": ">=2.0",
        "the-limbo/factory-contracts": "dev-master"
    },
    "provide": {
        "psr/container-implementation": ">=2.0",
        "the-limbo/factory-contracts-implementation": "dev-master"
    },
    "suggest": {
        "the-limbo/container-contracts-implementation": ""
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
